  <sqlMap namespace="studio.jgsgzbgl"> 
    <select id="dn1.jgsgzbglList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select * from( select $tjrq$ tjrq, jg.jgdh, jg.jgmc,zb.zbdh, zb.zbmc, bz.bzzwmc,dmms.dmms as sdbsmc,dmms1.dmms as tjkjmc,sgzb.zbz , case when sgzb.zbdh is null then '0' else '1' end as drzt ,qx.khdxdh,zbsd.sdbs,zbbz.bz,zbtjkj.tjkj from khdx_zb zb inner join khdx_zbsd zbsd on zbsd.zbdh=zb.zbdh inner join khdx_zbbz zbbz on zbbz.zbdh=zb.zbdh inner join csb_bz bz on bz.bz=zbbz.bz inner join csb_dmms dmms on dmms.dmz=zbsd.sdbs and dmms.dmmc='ZBSDBS' inner join table(fn_getsjjg_java_qx($tjrq$, $login_khdxdh$, 0)) qx on 1=1 inner join khdx_jg jg on jg.khdxdh=qx.khdxdh inner join khdx_zbtjkj zbtjkj on zbtjkj.zbdh=zb.zbdh and zbtjkj.tjkj in(0,1) inner join csb_dmms dmms1 on cast(dmms1.dmz as integer)=zbtjkj.tjkj and dmms1.dmmc='ZBTJKJ' left join yjzb_jgsgzb sgzb on sgzb.zbdh=zb.zbdh and sgzb.khdxdh=qx.khdxdh and zbbz.bz=sgzb.bz and sgzb.sdbs=zbsd.sdbs 
      <isNotEmpty property="tjrq">and sgzb.tjrq=$tjrq$</isNotEmpty> and cast(sgzb.tjkj as integer)=zbtjkj.tjkj where zb.whfs='2' 
      <isNotEmpty property="jgkhdxdh">and jg.khdxdh in ($jgkhdxdh$)</isNotEmpty>  
      <isNotEmpty property="zbdh">and zb.zbdh=$zbdh$</isNotEmpty> )a where 1=1 
      <isNotEmpty property="drzt">and a.drzt='$drzt$'</isNotEmpty> 
    </select>  
