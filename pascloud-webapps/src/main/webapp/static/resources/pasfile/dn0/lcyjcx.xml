  <sqlMap namespace="studio.lcyjcx">
    <select id="dn0.lcyjcxList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select  khdx_hy.khdxdh,        JXDX_LCJYLS.lsh,        JXDX_LCJYLS.jyrq,        JXDX_LCJYLS.zhdh,        JXDX_LCJYLS.khh,        JXDX_LCJYLS.khmc,        khlx.dmms as khlx,        cplb.dmms as cplb,        JXDX_LCCPXX.cpmc,        JXDX_LCJYLS.shrq,           KHDX_JG_alias.JGMC jyjg,        JXDX_LCJYLS.jyje,        JXDX_LCJYLS.jyfs,         jg.jgmc as hyssjg,        khdx_hy.hydh,        khdx_hy.hymc,               DXGX_YJGX_LC.qsrq,        DXGX_YJGX_LC.jsrq,        DXGX_YJGX_LC.zlbl fpbl         from DXGX_YJGX_LC     inner join JXDX_LCJYLS  on  DXGX_YJGX_LC.LSH = JXDX_LCJYLS.LSH  inner join khdx_hy  on DXGX_YJGX_LC.khdxdh = khdx_hy.khdxdh     inner join KHDX_JG KHDX_JG_alias on KHDX_JG_alias.JGDH = JXDX_LCJYLS.JGDH  inner join table(FN_GETSJJG_JAVA_QXHY($tjrq$, $login_khdxdh$, 0)) hyqx on khdx_hy.khdxdh = hyqx.khdxdh  inner join JXDX_LCCPXX on JXDX_LCCPXX.cpdm=JXDX_LCJYLS.cpdm  inner join khdx_jgcy cy on cy.khdxdh=khdx_hy.khdxdh
      <isNotEmpty property="tjrq">and $tjrq$ between cy.qsrq and cy.jsrq</isNotEmpty>
      inner join khdx_jg jg on cy.jgkhdxdh=jg.khdxdh  left  join csb_dmms khlx on khlx.dmz=JXDX_LCJYLS.khlx and khlx.dmmc='KHLX'  Left  join csb_dmms cplb on cplb.dmz=JXDX_LCCPXX.cplb and cplb.dmmc='LCCPLB'
      <isNotEmpty property="tjrq">where $tjrq$ between DXGX_YJGX_LC.qsrq and DXGX_YJGX_LC.jsrq</isNotEmpty>
      <isNotEmpty property="khdxdh">and khdx_hy.khdxdh in ($khdxdh$)</isNotEmpty>
      <isNotEmpty property="jyqsrq">and JXDX_LCJYLS.JYRQ between $jyqsrq$ and $jyjsrq$</isNotEmpty>
      <isNotEmpty property="lccplb">and JXDX_LCCPXX.cplb='$lccplb$'</isNotEmpty>
      <isNotEmpty property="khlx">and JXDX_LCJYLS.khlx='$khlx$'</isNotEmpty>
      <isNotEmpty property="khh">and JXDX_LCJYLS.khh like '%$khh$%'</isNotEmpty>
      <isNotEmpty property="khmc">and JXDX_LCJYLS.khmc like '%$khmc$%'</isNotEmpty>
    </select>
    <select id="dn0.lcyjcxCount" parameterClass="java.util.Map" resultClass="java.lang.Integer">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select count(1) from ( select  khdx_hy.khdxdh,        JXDX_LCJYLS.lsh,        JXDX_LCJYLS.jyrq,        JXDX_LCJYLS.zhdh,        JXDX_LCJYLS.khh,        JXDX_LCJYLS.khmc,        khlx.dmms as khlx,        cplb.dmms as cplb,        JXDX_LCCPXX.cpmc,        JXDX_LCJYLS.shrq,           KHDX_JG_alias.JGMC jyjg,        JXDX_LCJYLS.jyje,        JXDX_LCJYLS.jyfs,         jg.jgmc as hyssjg,        khdx_hy.hydh,        khdx_hy.hymc,               DXGX_YJGX_LC.qsrq,        DXGX_YJGX_LC.jsrq,        DXGX_YJGX_LC.zlbl fpbl         from DXGX_YJGX_LC     inner join JXDX_LCJYLS  on  DXGX_YJGX_LC.LSH = JXDX_LCJYLS.LSH  inner join khdx_hy  on DXGX_YJGX_LC.khdxdh = khdx_hy.khdxdh     inner join KHDX_JG KHDX_JG_alias on KHDX_JG_alias.JGDH = JXDX_LCJYLS.JGDH  inner join table(FN_GETSJJG_JAVA_QXHY($tjrq$, $login_khdxdh$, 0)) hyqx on khdx_hy.khdxdh = hyqx.khdxdh  inner join JXDX_LCCPXX on JXDX_LCCPXX.cpdm=JXDX_LCJYLS.cpdm  inner join khdx_jgcy cy on cy.khdxdh=khdx_hy.khdxdh
      <isNotEmpty property="tjrq">and $tjrq$ between cy.qsrq and cy.jsrq</isNotEmpty>
      inner join khdx_jg jg on cy.jgkhdxdh=jg.khdxdh  left  join csb_dmms khlx on khlx.dmz=JXDX_LCJYLS.khlx and khlx.dmmc='KHLX'  Left  join csb_dmms cplb on cplb.dmz=JXDX_LCCPXX.cplb and cplb.dmmc='LCCPLB'
      <isNotEmpty property="tjrq">where $tjrq$ between DXGX_YJGX_LC.qsrq and DXGX_YJGX_LC.jsrq</isNotEmpty>
      <isNotEmpty property="khdxdh">and khdx_hy.khdxdh in ($khdxdh$)</isNotEmpty>
      <isNotEmpty property="jyqsrq">and JXDX_LCJYLS.JYRQ between $jyqsrq$ and $jyjsrq$</isNotEmpty>
      <isNotEmpty property="lccplb">and JXDX_LCCPXX.cplb='$lccplb$'</isNotEmpty>
      <isNotEmpty property="khlx">and JXDX_LCJYLS.khlx='$khlx$'</isNotEmpty>
      <isNotEmpty property="khh">and JXDX_LCJYLS.khh like '%$khh$%'</isNotEmpty>
      <isNotEmpty property="khmc">and JXDX_LCJYLS.khmc like '%$khmc$%'</isNotEmpty>
      ) t
    </select>
