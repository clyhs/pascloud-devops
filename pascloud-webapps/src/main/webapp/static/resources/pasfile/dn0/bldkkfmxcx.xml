  <sqlMap namespace="studio.bldkkfmxcx">
    <select id="dn0.bldkkfmxcxList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select  a.khdxdh as khdxdh,$tjrq$ as tjrq,e.jgdh,e.jgmc,c.hydh,c.hymc,kh.khh,kh.khmc,         case when a.zhbs='1' then '对公' else '对私' end as khlx,         case when a.wjfl='1' then '正常' when a.wjfl='2' then '关注' when a.wjfl='3' then '次级' when a.wjfl='4' then '可疑' when a.wjfl='5' then '损失'else  '正常' end as wjfl,         case when a.sjfl='1' then '正常' when a.sjfl='2' then '逾期' when a.sjfl='3' then '呆滞' when a.sjfl='4' then '呆账' else  '正常' end as sjfl,         b.zhdh,a.DKJE,a.ZLBL,a.zhye,a.hyye,a.kfbz as kfbz,a.yfgz,a.sfgz,a.jxdxdh,a.gzfamc    from dkjx_bldkkfmx a   inner join jxdx_dkzh b on a.jxdxdh=b.jxdxdh   inner join khdx_hy  c on c.khdxdh=a.khdxdh   inner join table(FN_GETSJJG_JAVA_QXHY($tjrq$, $login_khdxdh$, 0)) hyqx on b.khdxdh = hyqx.khdxdh    inner join khdx_jgcy d on d.khdxdh=c.khdxdh and a.tjrq between d.qsrq and d.jsrq inner join khdx_jg e on e.khdxdh=d.jgkhdxdh   inner join khdx_kh kh on kh.khh=a.khh   where 1=1
      <isNotEmpty property="tjrq">and  a.tjrq=$tjrq$</isNotEmpty>
      <isNotEmpty property="khdxdh">and a.khdxdh in  ($khdxdh$)</isNotEmpty>
      <isNotEmpty property="wjfl">and a.wjfl= '$wjfl$'</isNotEmpty>
      <isNotEmpty property="sjfl">and a.sjfl= '$sjfl$'</isNotEmpty>
      <isNotEmpty property="zhbs">and a.zhbs='$zhbs$'</isNotEmpty>
      <isNotEmpty property="khh">and a.khh like '%$khh$%'</isNotEmpty>
      <isNotEmpty property="khmc">and kh.khmc like '%$khmc$%'</isNotEmpty>
      <isNotEmpty property="zhdh">and b.zhdh like '%$zhdh$%'</isNotEmpty>
      <isNotEmpty property="zhyeqjxx">and a.zhye between  $zhyeqjxx$ and $zhyeqjsx$</isNotEmpty>
      and a.wjfl in ('3','4','5')     and a.sjfl in ('2','3','4')   order by c.hydh,e.jgdh
    </select>
    <select id="dn0.bldkkfmxcxCount" parameterClass="java.util.Map" resultClass="java.lang.Integer">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select count(1) from ( select  a.khdxdh as khdxdh,$tjrq$ as tjrq,e.jgdh,e.jgmc,c.hydh,c.hymc,kh.khh,kh.khmc,         case when a.zhbs='1' then '对公' else '对私' end as khlx,         case when a.wjfl='1' then '正常' when a.wjfl='2' then '关注' when a.wjfl='3' then '次级' when a.wjfl='4' then '可疑' when a.wjfl='5' then '损失'else  '正常' end as wjfl,         case when a.sjfl='1' then '正常' when a.sjfl='2' then '逾期' when a.sjfl='3' then '呆滞' when a.sjfl='4' then '呆账' else  '正常' end as sjfl,         b.zhdh,a.DKJE,a.ZLBL,a.zhye,a.hyye,a.kfbz as kfbz,a.yfgz,a.sfgz,a.jxdxdh,a.gzfamc    from dkjx_bldkkfmx a   inner join jxdx_dkzh b on a.jxdxdh=b.jxdxdh   inner join khdx_hy  c on c.khdxdh=a.khdxdh   inner join table(FN_GETSJJG_JAVA_QXHY($tjrq$, $login_khdxdh$, 0)) hyqx on b.khdxdh = hyqx.khdxdh    inner join khdx_jgcy d on d.khdxdh=c.khdxdh and a.tjrq between d.qsrq and d.jsrq inner join khdx_jg e on e.khdxdh=d.jgkhdxdh   inner join khdx_kh kh on kh.khh=a.khh   where 1=1
      <isNotEmpty property="tjrq">and  a.tjrq=$tjrq$</isNotEmpty>
      <isNotEmpty property="khdxdh">and a.khdxdh in  ($khdxdh$)</isNotEmpty>
      <isNotEmpty property="wjfl">and a.wjfl= '$wjfl$'</isNotEmpty>
      <isNotEmpty property="sjfl">and a.sjfl= '$sjfl$'</isNotEmpty>
      <isNotEmpty property="zhbs">and a.zhbs='$zhbs$'</isNotEmpty>
      <isNotEmpty property="khh">and a.khh like '%$khh$%'</isNotEmpty>
      <isNotEmpty property="khmc">and kh.khmc like '%$khmc$%'</isNotEmpty>
      <isNotEmpty property="zhdh">and b.zhdh like '%$zhdh$%'</isNotEmpty>
      <isNotEmpty property="zhyeqjxx">and a.zhye between  $zhyeqjxx$ and $zhyeqjsx$</isNotEmpty>
      and a.wjfl in ('3','4','5')     and a.sjfl in ('2','3','4')   order by c.hydh,e.jgdh ) t
    </select>
