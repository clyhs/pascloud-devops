      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select $tjrq$ as tjrq,a.khdxdh,c.jgdh,c.jgmc,b.hydh,b.hymc,d.lbmc as hylb,e.gwmc as hygw, a.gzjs,round(a.gwxs,4) gwxs,round(a.yfgz,2) yfgwgz,round(a.sfgz,2) as sfgwgz       from jbgz_gwgz a       inner join khdx_hy b on b.khdxdh=a.khdxdh       inner join khdx_jg c on c.jgdh=a.jgdh       inner join csb_hylb d on d.lbdh=a.hylb        inner join csb_hygw e on e.gwbh=a.hygw       inner join table(fn_getsjjg_java_qxhy($tjrq$,$login_khdxdh$,0)) hy on hy.khdxdh=b.khdxdh
