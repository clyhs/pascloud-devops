  <sqlMap namespace="studio.jgdjpdtz">
    <select id="dn0.jgdjpdtzList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select djpd_yyjg_jg.tjrq, djpd_yyjg_jg.khdxdh, djpd_yyjg_jg.bh, djpd_yyjg_jg.djpdmc olddjpdmc, khdx_jg.jgdh, khdx_jg.jgmc, jglb.lbmc jglb, dmms.dmms khzq, djpd_yyjg_jg.pm, djpd_yyjg_jg.dj olddj, djpd_yyjg_jg.jbmc oldjbmc, djpd_yyjg_jg.jlbz oldjlbz, djpd_yyjg_jg.pdlb oldpdlb, djpd_yyjg_jg.ms   oldms, DJPD_YYMX_JG.xmmc, cast(DJPD_YYMX_JG.xmz as decimal(19,2)) as xmz from djpd_yyjg_jg djpd_yyjg_jg inner join khdx_jg khdx_jg on khdx_jg.khdxdh=djpd_yyjg_jg.khdxdh             inner join csb_dmms dmms on upper(dmms.dmmc) = 'DJPDZQ'              and dmms.dmz = djpd_yyjg_jg.pdzq inner join khdx_jglb jglb on djpd_yyjg_jg.khdxdh=jglb.khdxdh and djpd_yyjg_jg.tjrq between jglb.qsrq and jglb.jsrq inner join DJPD_YYMX_JG DJPD_YYMX_JG on DJPD_YYMX_JG.tjrq=djpd_yyjg_jg.tjrq and DJPD_YYMX_JG.bh=djpd_yyjg_jg.bh and DJPD_YYMX_JG.khdxdh=djpd_yyjg_jg.khdxdh   inner join table(fn_getsjjg_java_qx($pdrq$, $login_khdxdh$, 0)) qx on qx.khdxdh = khdx_jg.khdxdh   where 1=1
      <isNotEmpty property="pdrq">and  djpd_yyjg_jg.tjrq= $pdrq$</isNotEmpty>
      <isNotEmpty property="jgkhdxdh">and khdx_jg.khdxdh in ($jgkhdxdh$)</isNotEmpty>
      <isNotEmpty property="djpdmc">and djpd_yyjg_jg.djpdmc = '$djpdmc$'</isNotEmpty>
      order by djpd_yyjg_jg.pm asc
    </select>
    <select id="dn0.jgdjpdtzCount" parameterClass="java.util.Map" resultClass="java.lang.Integer">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select count(1) from ( select djpd_yyjg_jg.tjrq, djpd_yyjg_jg.khdxdh, djpd_yyjg_jg.bh, djpd_yyjg_jg.djpdmc olddjpdmc, khdx_jg.jgdh, khdx_jg.jgmc, jglb.lbmc jglb, dmms.dmms khzq, djpd_yyjg_jg.pm, djpd_yyjg_jg.dj olddj, djpd_yyjg_jg.jbmc oldjbmc, djpd_yyjg_jg.jlbz oldjlbz, djpd_yyjg_jg.pdlb oldpdlb, djpd_yyjg_jg.ms   oldms, DJPD_YYMX_JG.xmmc, cast(DJPD_YYMX_JG.xmz as decimal(19,2)) as xmz from djpd_yyjg_jg djpd_yyjg_jg inner join khdx_jg khdx_jg on khdx_jg.khdxdh=djpd_yyjg_jg.khdxdh             inner join csb_dmms dmms on upper(dmms.dmmc) = 'DJPDZQ'              and dmms.dmz = djpd_yyjg_jg.pdzq inner join khdx_jglb jglb on djpd_yyjg_jg.khdxdh=jglb.khdxdh and djpd_yyjg_jg.tjrq between jglb.qsrq and jglb.jsrq inner join DJPD_YYMX_JG DJPD_YYMX_JG on DJPD_YYMX_JG.tjrq=djpd_yyjg_jg.tjrq and DJPD_YYMX_JG.bh=djpd_yyjg_jg.bh and DJPD_YYMX_JG.khdxdh=djpd_yyjg_jg.khdxdh   inner join table(fn_getsjjg_java_qx($pdrq$, $login_khdxdh$, 0)) qx on qx.khdxdh = khdx_jg.khdxdh   where 1=1
      <isNotEmpty property="pdrq">and  djpd_yyjg_jg.tjrq= $pdrq$</isNotEmpty>
      <isNotEmpty property="jgkhdxdh">and khdx_jg.khdxdh in ($jgkhdxdh$)</isNotEmpty>
      <isNotEmpty property="djpdmc">and djpd_yyjg_jg.djpdmc = '$djpdmc$'</isNotEmpty>
      order by djpd_yyjg_jg.pm asc ) t
    </select>
    <select id="dn0.jgdjpdtz_select_by_primaryKey" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
