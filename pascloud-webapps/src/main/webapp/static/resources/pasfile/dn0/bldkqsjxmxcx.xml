  <sqlMap namespace="studio.bldkqsjxmxcx">
    <select id="dn0.bldkqsjxmxcxList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select a.khdxdh,a.qsrq,d.JGDH,d.jgmc,b.hydh,       b.hymc,a.zhdh,a.ZHHM,a.ffrq,        dmms1.dmms as wjfl,       qx.qxmc,a.yqrq,dm.dmms as bllx,dmm.dmms as qsfs,round(a.shbj,2) shbj,a.bjjtbl,       a.SHLX,a.LXJTBL,round(a.jtgz,2) as jlje,a.bzsm from  sglr_bldkqsmx a      inner join khdx_hy b on b.khdxdh=a.khdxdh     inner join khdx_hylb hl on b.khdxdh=hl.khdxdh and a.qsrq between  hl.qsrq and hl.jsrq inner join khdx_jgcy c on c.khdxdh=b.khdxdh and a.qsrq between  c.qsrq and c.jsrq inner join khdx_jg d on d.khdxdh=c.jgkhdxdh      left join CSB_DMMS dmms1 on dmms1.dmz=a.wjfl and upper(dmms1.dmmc)='BLWJFL'     left join csb_dmms dm on dm.DMZ=a.BLLX and dm.DMMC='BLLX'     left join csb_dmms dmm on dmm.DMZ=a.qsfs and dmm.DMMC='QSFS'     inner join csb_qxsdb qx on qx.jxdxlx=2 and a.dkqx between qx.qsqx and qx.jsqx inner join table(fn_getsjjg_java_qxhy($qsjsrq$,$login_khdxdh$,0)) hy on hy.khdxdh=b.khdxdh
      <isNotEmpty property="khdxdh">where a.khdxdh in ($khdxdh$)</isNotEmpty>
      <isNotEmpty property="qsqsrq">and a.qsrq between $qsqsrq$ and $qsjsrq$</isNotEmpty>
      <isNotEmpty property="hylb">and hl.lbdh='$hylb$'</isNotEmpty>
      order by c.jgdh,b.hydh
    </select>
    <select id="dn0.bldkqsjxmxcxCount" parameterClass="java.util.Map" resultClass="java.lang.Integer">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select count(1) from ( select a.khdxdh,a.qsrq,d.JGDH,d.jgmc,b.hydh,       b.hymc,a.zhdh,a.ZHHM,a.ffrq,        dmms1.dmms as wjfl,       qx.qxmc,a.yqrq,dm.dmms as bllx,dmm.dmms as qsfs,round(a.shbj,2) shbj,a.bjjtbl,       a.SHLX,a.LXJTBL,round(a.jtgz,2) as jlje,a.bzsm from  sglr_bldkqsmx a      inner join khdx_hy b on b.khdxdh=a.khdxdh     inner join khdx_hylb hl on b.khdxdh=hl.khdxdh and a.qsrq between  hl.qsrq and hl.jsrq inner join khdx_jgcy c on c.khdxdh=b.khdxdh and a.qsrq between  c.qsrq and c.jsrq inner join khdx_jg d on d.khdxdh=c.jgkhdxdh      left join CSB_DMMS dmms1 on dmms1.dmz=a.wjfl and upper(dmms1.dmmc)='BLWJFL'     left join csb_dmms dm on dm.DMZ=a.BLLX and dm.DMMC='BLLX'     left join csb_dmms dmm on dmm.DMZ=a.qsfs and dmm.DMMC='QSFS'     inner join csb_qxsdb qx on qx.jxdxlx=2 and a.dkqx between qx.qsqx and qx.jsqx inner join table(fn_getsjjg_java_qxhy($qsjsrq$,$login_khdxdh$,0)) hy on hy.khdxdh=b.khdxdh
      <isNotEmpty property="khdxdh">where a.khdxdh in ($khdxdh$)</isNotEmpty>
      <isNotEmpty property="qsqsrq">and a.qsrq between $qsqsrq$ and $qsjsrq$</isNotEmpty>
