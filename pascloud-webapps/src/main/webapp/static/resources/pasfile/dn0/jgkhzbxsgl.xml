    <select id="dn1.jgkhzbxsglList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select zb.khzbdh,zb.khzbmc,jg.jglb jglb, (case when jg.jglb='-1' then '全行' else lb.lbmc end) lbmc,jg.bzxs,jg.cjhxs,jg.clzxs,jg.qsrq,jg.jsrq ,'2' as yyzlbh from csb_khzbxs_jg jg inner join khfa_khzb_jg zb on jg.khzbdh = zb.khzbdh left join csb_jglb lb on jg.jglb = lb.lbdh where 1=1 
      <isNotEmpty property="khzb">and zb.khzbdh = $khzb$</isNotEmpty>  
      <isNotEmpty property="jglb1">and jg.jglb ='$jglb1$'</isNotEmpty> and jg.yyzlbh=2
    </select>  
