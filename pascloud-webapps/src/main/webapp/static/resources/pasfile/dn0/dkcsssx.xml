//自定义验证，在提交时执行。通过返回true，否则返回false
function customValidate(){
 var zhye1=$('#zhye1').val();
 var zhye2=$('#zhye2').val();
 if(zhye1>zhye2){
   alert("账户余额下限不能大于账户余额上限！");
   return false;
 }
 return true;
}


function openWMX(obj){  
    var pars = "{khdxdh:'"+obj['KHDXDH']+"',tjrq:'"+obj['TJRQ']+"',gzxbh:'"+obj['GZXBH']+"',yyzl:'"+1+"'}";
    //alert(pars);
    querySubQuery(pars,'hygzxzbjgcx','45','30','tag');

}

function gzxsf_select(obj){
  if(obj.data.length<1){
    $('#gzxsf').val(1);
  }else{
    if(obj.data[0].JXDXDH==-1){
      $('#gzxsf').val(2);
    }else{
      $('#gzxsf').val(1);
    }
  }
  
}]]></javascript>
