    <select id="dn0.hyfxjjtmxcxList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select fxjmx.tjrq  as tjrq,jg.jgdh,jg.jgmc,hy.hydh,hy.hymc, dkzh.khh,dkzh.zhhm,dmms.dmms as khlx, dkzh.zhdh,dkzh.ffrq,dkzh.dqrq,fxjmx.dyjtfxj,fxjmx.ljyjtfxj,fxjmx.yfgz ,fxjmx.sfgz ,gzx.gzxmc as fxjlx from dkjx_dkfxjjtmx fxjmx left join gztx_gzxdy gzx on fxjmx.dygzxbh=gzx.gzxbh inner join khdx_hy hy on hy.khdxdh = fxjmx.khdxdh inner join khdx_jgcy jgcy on jgcy.khdxdh = hy.khdxdh
