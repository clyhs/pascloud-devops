      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select count(1) from ( select zb.khzbdh,zb.khzbmc, xs.jglb jglb, (case when xs.jglb='-1' then '全行' else jlb.lbmc end) jglbmc, xs.hylb hylb, (case when xs.hylb='-1' then '全行' else hlb.lbmc end) hylbmc, xs.bzxs,xs.kfdj,xs.qsrq,xs.jsrq,'1' as yyzlbh from csb_khzbxs_hy xs inner join khfa_khzb_hy zb on xs.khzbdh = zb.khzbdh left join csb_jglb jlb on xs.jglb=jlb.lbdh left join csb_hylb hlb on xs.hylb=hlb.lbdh where 1=1 
      <isNotEmpty property="khzb">and zb.khzbdh = $khzb$</isNotEmpty>  
