  <sqlMap namespace="studio.dktjjx">
    <select id="dn12.dktjjxList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select $tjrq$ as tjrq,jg.jgdh,jg.jgmc,hy.hydh,hy.hymc,lb.lbmc,dk.zhdh,dk.zhhm,ms.dmms as zhbs,dk.ffrq,dk.dkje, gz.jcdj as jcbz, js.dmms as dkjs, gz.fpbl, gz.jlje, gz.khdxdh, gz.jxdxdh from JXGZ_TJFDJXMX gz   inner join jxdx_dkzh dk on gz.jxdxdh=dk.jxdxdh inner join khdx_hy hy on gz.khdxdh=hy.KHDXDH inner join khdx_hylb lb on hy.KHDXDH=lb.khdxdh
      <isNotEmpty property="tjrq_ym">and  $tjrq_ym$ between lb.qsrq and lb.JSRQ</isNotEmpty>
      inner join khdx_jgcy cy on hy.khdxdh=cy.KHDXDH
      <isNotEmpty property="tjrq_ym">and $tjrq_ym$ between cy.qsrq and cy.JSRQ</isNotEmpty>
      inner join khdx_jg jg on cy.jgkhdxdh=jg.khdxdh left join csb_dmms ms on dk.zhbs=ms.dmz and ms.DMMC='ZHBS' left join csb_dmms js on gz.fpjs=js.dmz and js.dmmc='DKJS' inner join table(FN_GETSJJG_JAVA_QXHY($tjrq_ym$, $login_khdxdh$, 0)) hyqx on hy.khdxdh = hyqx.khdxdh
      <isNotEmpty property="khdxdh">where hy.khdxdh in ($khdxdh$)</isNotEmpty>
      <isNotEmpty property="dkjs">and gz.fpjs in ($dkjs$)</isNotEmpty>
      <isNotEmpty property="tjrq_ym">and gz.tjrq=$tjrq_ym$</isNotEmpty>
      <isNotEmpty property="zhbs">and dk.zhbs=$zhbs$</isNotEmpty>
      order by jg.jgdh,hy.hydh,gz.jxdxdh
    </select>
    <select id="dn12.dktjjxCount" parameterClass="java.util.Map" resultClass="java.lang.Integer">
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty>
      select count(1) from ( select $tjrq$ as tjrq,jg.jgdh,jg.jgmc,hy.hydh,hy.hymc,lb.lbmc,dk.zhdh,dk.zhhm,ms.dmms as zhbs,dk.ffrq,dk.dkje, gz.jcdj as jcbz, js.dmms as dkjs, gz.fpbl, gz.jlje, gz.khdxdh, gz.jxdxdh from JXGZ_TJFDJXMX gz   inner join jxdx_dkzh dk on gz.jxdxdh=dk.jxdxdh inner join khdx_hy hy on gz.khdxdh=hy.KHDXDH inner join khdx_hylb lb on hy.KHDXDH=lb.khdxdh
      <isNotEmpty property="tjrq_ym">and  $tjrq_ym$ between lb.qsrq and lb.JSRQ</isNotEmpty>
      inner join khdx_jgcy cy on hy.khdxdh=cy.KHDXDH
      <isNotEmpty property="tjrq_ym">and $tjrq_ym$ between cy.qsrq and cy.JSRQ</isNotEmpty>
      inner join khdx_jg jg on cy.jgkhdxdh=jg.khdxdh left join csb_dmms ms on dk.zhbs=ms.dmz and ms.DMMC='ZHBS' left join csb_dmms js on gz.fpjs=js.dmz and js.dmmc='DKJS' inner join table(FN_GETSJJG_JAVA_QXHY($tjrq_ym$, $login_khdxdh$, 0)) hyqx on hy.khdxdh = hyqx.khdxdh
      <isNotEmpty property="khdxdh">where hy.khdxdh in ($khdxdh$)</isNotEmpty>
      <isNotEmpty property="dkjs">and gz.fpjs in ($dkjs$)</isNotEmpty>
      <isNotEmpty property="tjrq_ym">and gz.tjrq=$tjrq_ym$</isNotEmpty>
      <isNotEmpty property="zhbs">and dk.zhbs=$zhbs$</isNotEmpty>
      order by jg.jgdh,hy.hydh,gz.jxdxdh ) t
    </select>
