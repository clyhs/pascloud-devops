  <sqlMap namespace="studio.jxkhcspz"> 
    <select id="dn0.jxkhcspzList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select (case when khcs.jglb='-1' then '全行' else jglb.lbmc end) as jglbmc, (case when khcs.jglb='-1' then '-1' else jglb.lbdh end) as jglb, (case when khcs.hylb='-1' then '全行' else hylb.lbmc end) as hylbmc, (case when khcs.hylb='-1' then '-1' else hylb.lbdh end) as hylb, khcs.csdh,ms.csmc,khcs.qsrq,khcs.jsrq,khcs.csz,ms.csdw,ms.cssm from csb_jxkhcs khcs left join csb_jglb jglb on jglb.lbdh = khcs.jglb left join csb_hylb hylb on hylb.lbdh = khcs.hylb inner join csb_jxkhcsmc ms on ms.csdh = khcs.csdh where 1=1 
      <isNotEmpty property="jglb">and khcs.jglb = '$jglb$'</isNotEmpty>  
      <isNotEmpty property="hylb">and khcs.hylb = '$hylb$'</isNotEmpty>  
      <isNotEmpty property="csdh">and ms.csdh = '$csdh$'</isNotEmpty> order by khcs.csdh,khcs.jglb,khcs.hylb
    </select>  
    <select id="dn0.jxkhcspzCount" parameterClass="java.util.Map" resultClass="java.lang.Integer"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select count(1) from ( select (case when khcs.jglb='-1' then '全行' else jglb.lbmc end) as jglbmc, (case when khcs.jglb='-1' then '-1' else jglb.lbdh end) as jglb, (case when khcs.hylb='-1' then '全行' else hylb.lbmc end) as hylbmc, (case when khcs.hylb='-1' then '-1' else hylb.lbdh end) as hylb, khcs.csdh,ms.csmc,khcs.qsrq,khcs.jsrq,khcs.csz,ms.csdw,ms.cssm from csb_jxkhcs khcs left join csb_jglb jglb on jglb.lbdh = khcs.jglb left join csb_hylb hylb on hylb.lbdh = khcs.hylb inner join csb_jxkhcsmc ms on ms.csdh = khcs.csdh where 1=1 
      <isNotEmpty property="jglb">and khcs.jglb = '$jglb$'</isNotEmpty>  
      <isNotEmpty property="hylb">and khcs.hylb = '$hylb$'</isNotEmpty>  
      <isNotEmpty property="csdh">and ms.csdh = '$csdh$'</isNotEmpty> order by khcs.csdh,khcs.jglb,khcs.hylb ) t
    </select>  
