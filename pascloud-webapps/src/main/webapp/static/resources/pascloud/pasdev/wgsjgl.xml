  <sqlMap namespace="studio.wgsjgl"> 
    <select id="dn1.wgsjglList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select dj.wgrq,dj.sjbh, wg.hydh as wghydh, wg.hymc as wghymc, fl.wgxmc, wgxm.xmmc, dj.kf, dj.wgms, czqk.dmms as czqk, dj.czrq, czjg.dmms as czjg, dj.czms, zrr.hymc as zrrhymc, jbr.hymc as jbrhymc, qrr.hymc as qrrhymc, case when dj.qrzt='0' then '未确认' when dj.qrzt='1' then '确认' end qrzt from wgjf_wgsjdj dj inner join wgjf_wgxm wgxm on wgxm.wgxmdh = dj.wgxmdh inner join wgjf_wgxmfl fl on wgxm.wgxlb = fl.wgxlb and dj.wgrq between fl.qsrq and fl.jsrq inner join khdx_hy wg on wg.khdxdh = dj.wghy inner join khdx_hy zrr on zrr.khdxdh = dj.clzrr inner join khdx_hy jbr on jbr.khdxdh = dj.jbr left join khdx_hy qrr on qrr.hydh = dj.qrr inner join csb_dmms czqk on czqk.dmz = dj.czqk and czqk.dmmc = 'CZQK' inner join csb_dmms czjg on czjg.dmz = dj.czjg and czjg.dmmc = 'CZJG' where 1=1 
      <isNotEmpty property="wgqsrq">and dj.wgrq between $wgqsrq$ and $wgjsrq$</isNotEmpty>  
      <isNotEmpty property="wgxm">and dj.wgxmdh = '$wgxm$'</isNotEmpty>  
      <isNotEmpty property="wghy">and dj.wghy = $wghy$</isNotEmpty>  
      <isNotEmpty property="clzrr">and dj.clzrr = $clzrr$</isNotEmpty>  
      <isNotEmpty property="czqk">and dj.czqk = '$czqk$'</isNotEmpty>  
      <isNotEmpty property="czjg">and dj.czjg = '$czjg$'</isNotEmpty> 
    </select>  
    <select id="dn1.wgsjglCount" parameterClass="java.util.Map" resultClass="java.lang.Integer"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select count(1) from ( select dj.wgrq,dj.sjbh, wg.hydh as wghydh, wg.hymc as wghymc, fl.wgxmc, wgxm.xmmc, dj.kf, dj.wgms, czqk.dmms as czqk, dj.czrq, czjg.dmms as czjg, dj.czms, zrr.hymc as zrrhymc, jbr.hymc as jbrhymc, qrr.hymc as qrrhymc, case when dj.qrzt='0' then '未确认' when dj.qrzt='1' then '确认' end qrzt from wgjf_wgsjdj dj inner join wgjf_wgxm wgxm on wgxm.wgxmdh = dj.wgxmdh inner join wgjf_wgxmfl fl on wgxm.wgxlb = fl.wgxlb and dj.wgrq between fl.qsrq and fl.jsrq inner join khdx_hy wg on wg.khdxdh = dj.wghy inner join khdx_hy zrr on zrr.khdxdh = dj.clzrr inner join khdx_hy jbr on jbr.khdxdh = dj.jbr left join khdx_hy qrr on qrr.hydh = dj.qrr inner join csb_dmms czqk on czqk.dmz = dj.czqk and czqk.dmmc = 'CZQK' inner join csb_dmms czjg on czjg.dmz = dj.czjg and czjg.dmmc = 'CZJG' where 1=1 
