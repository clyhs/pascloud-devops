    <select id="dn0.hysgzbglList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select * from( select $tjrq$ tjrq, jg.jgdh, jg.jgmc, qx.hydh, qx.hymc, zb.zbdh, zb.zbmc, bz.bzzwmc, dmms.dmms, sgzb.zbz , case when sgzb.zbdh is null then '0' else '1' end as drzt ,qx.khdxdh,zbsd.sdbs,zbbz.bz from khdx_zb zb inner join khdx_zbsd zbsd on zbsd.zbdh=zb.zbdh inner join khdx_zbbz zbbz on zbbz.zbdh=zb.zbdh inner join csb_bz bz on bz.bz=zbbz.bz inner join csb_dmms dmms on dmms.dmz=zbsd.sdbs and dmms.dmmc='ZBSDBS' inner join table(fn_getsjjg_java_qxhy($tjrq$, $login_khdxdh$, 0)) qx on 1=1 inner join khdx_jgcy jgcy on jgcy.khdxdh=qx.khdxdh 
      <isNotEmpty property="tjrq">and $tjrq$ between jgcy.qsrq and jgcy.jsrq</isNotEmpty> inner join khdx_jg jg on jg.khdxdh=jgcy.jgkhdxdh left join yjzb_hysgzb sgzb on sgzb.zbdh=zb.zbdh and sgzb.khdxdh=qx.khdxdh and zbbz.bz=sgzb.bz and sgzb.sdbs=zbsd.sdbs 
      <isNotEmpty property="tjrq">and sgzb.tjrq=$tjrq$</isNotEmpty> where zb.whfs='2' 
      <isNotEmpty property="khdxdh">and jgcy.khdxdh in ($khdxdh$)</isNotEmpty>  
      <isNotEmpty property="zbdh">and zb.zbdh=$zbdh$</isNotEmpty> )a where 1=1 
      <isNotEmpty property="drzt">and a.drzt='$drzt$'</isNotEmpty> 
    </select>  
