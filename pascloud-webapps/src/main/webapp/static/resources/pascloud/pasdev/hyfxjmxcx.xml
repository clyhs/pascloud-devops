      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select substr(cast(fxjmx.tjrq as char(8)),1,6) as tjrq,jg.jgdh,jg.jgmc,hy.hydh,hy.hymc,dkzh.khh,dkzh.zhhm,dmms.dmms as khlx, dkzh.zhdh,dkzh.ffrq,dkzh.dqrq,fxjmx.ljyjtfxj,fxjmx.yfhfxj,fxjmx.dfhfxj from dkjx_dkfxjmx fxjmx inner join khdx_hy hy on hy.khdxdh = fxjmx.khdxdh inner join khdx_jgcy jgcy on jgcy.khdxdh = hy.khdxdh 
