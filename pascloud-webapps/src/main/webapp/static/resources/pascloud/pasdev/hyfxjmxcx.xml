      <isNotEmpty property="tjrq">where fxjmx.tjrq between $tjrq$01 and $tjrq_ym$</isNotEmpty>  
      <isNotEmpty property="khdxdh">and hy.khdxdh in ($khdxdh$)</isNotEmpty>  
