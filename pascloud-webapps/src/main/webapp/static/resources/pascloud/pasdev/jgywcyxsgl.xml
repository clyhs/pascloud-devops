  <sqlMap namespace="studio.jgywcyxsgl"> 
    <select id="dn0.jgywcyxsglList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select cs.khdxdh ,cs.ywlb ,cs.qsrq,cs.jsrq,cs.cyxs ,jg.jgmc,yw.ms as ywlbmc from csb_jgywcyxs cs inner join khdx_jg jg on jg.khdxdh=cs.khdxdh inner join csb_ywlb yw on yw.ywlb=cs.ywlb where 1=1 
      <isNotEmpty property="ywlb">and cs.ywlb='$ywlb$'</isNotEmpty> and exists(select 1 from table(fn_getsjjg_java_qx($sysdate$,$login_khdxdh$,$jgkhdxdh$)) qx where qx.khdxdh=jg.khdxdh ) order by jg.jgdh
    </select>  
    <select id="dn0.jgywcyxsglCount" parameterClass="java.util.Map" resultClass="java.lang.Integer"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select count(1) from ( select cs.khdxdh ,cs.ywlb ,cs.qsrq,cs.jsrq,cs.cyxs ,jg.jgmc,yw.ms as ywlbmc from csb_jgywcyxs cs inner join khdx_jg jg on jg.khdxdh=cs.khdxdh inner join csb_ywlb yw on yw.ywlb=cs.ywlb where 1=1 
      <isNotEmpty property="ywlb">and cs.ywlb='$ywlb$'</isNotEmpty> and exists(select 1 from table(fn_getsjjg_java_qx($sysdate$,$login_khdxdh$,$jgkhdxdh$)) qx where qx.khdxdh=jg.khdxdh ) order by jg.jgdh ) t
    </select>  
