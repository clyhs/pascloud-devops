  <sqlMap namespace="studio.hyfxzbgl"> 
    <select id="dn0.hyfxzbglList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select fxzb.fxzbdh,fxzb.fxzbmc,fxzb.zbdh as jczbdh,zb.zbmc as jczbmc,fxzb.sdbs, dmms1.dmms as sdbsmc,fxzb.bz,bz.bzzwmc,fxzb.tjkj,fxzb.zbpx from khfa_fxzb_hy fxzb inner join khdx_zblb zblb on zblb.zbdh=fxzb.zbdh and zblb.jsrq=29991231 inner join khdx_zb zb on zb.zbdh=zblb.zbdh left join csb_dmms dmms1 on lower(dmms1.dmmc)='zbsdbs' and dmms1.dmz=fxzb.sdbs left join csb_bz bz on fxzb.bz = bz.bz where 1=1 
      <isNotEmpty property="fxzbmc">and fxzb.fxzbmc like '%$fxzbmc$%'</isNotEmpty>  
      <isNotEmpty property="ywlb">and zblb.ywlb='$ywlb$'</isNotEmpty> order by zbpx
    </select>  
    <select id="dn0.hyfxzbglCount" parameterClass="java.util.Map" resultClass="java.lang.Integer"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select count(1) from ( select fxzb.fxzbdh,fxzb.fxzbmc,fxzb.zbdh as jczbdh,zb.zbmc as jczbmc,fxzb.sdbs, dmms1.dmms as sdbsmc,fxzb.bz,bz.bzzwmc,fxzb.tjkj,fxzb.zbpx from khfa_fxzb_hy fxzb inner join khdx_zblb zblb on zblb.zbdh=fxzb.zbdh and zblb.jsrq=29991231 inner join khdx_zb zb on zb.zbdh=zblb.zbdh left join csb_dmms dmms1 on lower(dmms1.dmmc)='zbsdbs' and dmms1.dmz=fxzb.sdbs left join csb_bz bz on fxzb.bz = bz.bz where 1=1 
      <isNotEmpty property="fxzbmc">and fxzb.fxzbmc like '%$fxzbmc$%'</isNotEmpty>  
      <isNotEmpty property="ywlb">and zblb.ywlb='$ywlb$'</isNotEmpty> order by zbpx ) t
    </select>  
