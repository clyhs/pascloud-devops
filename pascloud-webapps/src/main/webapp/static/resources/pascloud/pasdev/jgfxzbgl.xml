function checkFxzbmcBack(obj){
  if(obj.data.length&gt;0){
    alert('该分析指标名称已经存在!');
  }else{
    PAS.di.setDone(obj.sqlid);
  }
}

function getFxzbdhBack(obj){
  if(obj.data.length&gt;0){
    jQuery('#fxzbdh').val(obj.data[0].ID);
    PAS.di.setDone(obj.sqlid);
  }
