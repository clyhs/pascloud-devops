  <sqlMap namespace="studio.gysgywlgl"> 
    <select id="dn1.gysgywlglList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select yw.tjrq,yw.khdxdh,yw.jydm,yw.ywl,dm.jymc,hy.hymc,substr(yw.tjrq,1,6) as tjny from sglr_hysgywl yw inner join khdx_hy hy on hy.khdxdh=yw.khdxdh inner join csb_hxjydm dm on dm.jydm=yw.jydm inner join table(fn_getsjjg_java_qxhy($tjrq_ym$,$login_khdxdh$,0)) qx on qx.khdxdh=hy.khdxdh 
      <isNotEmpty property="khdxdh">where yw.khdxdh in ($khdxdh$)</isNotEmpty>  
      <isNotEmpty property="tjrq_ym">and yw.tjrq=$tjrq_ym$</isNotEmpty>  
      <isNotEmpty property="jydm">and yw.jydm like '%$jydm$%'</isNotEmpty> 
    </select>  
    <select id="dn1.gysgywlglCount" parameterClass="java.util.Map" resultClass="java.lang.Integer"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select count(1) from ( select yw.tjrq,yw.khdxdh,yw.jydm,yw.ywl,dm.jymc,hy.hymc,substr(yw.tjrq,1,6) as tjny from sglr_hysgywl yw inner join khdx_hy hy on hy.khdxdh=yw.khdxdh inner join csb_hxjydm dm on dm.jydm=yw.jydm inner join table(fn_getsjjg_java_qxhy($tjrq_ym$,$login_khdxdh$,0)) qx on qx.khdxdh=hy.khdxdh 
      <isNotEmpty property="khdxdh">where yw.khdxdh in ($khdxdh$)</isNotEmpty>  
      <isNotEmpty property="tjrq_ym">and yw.tjrq=$tjrq_ym$</isNotEmpty>  
      <isNotEmpty property="jydm">and yw.jydm like '%$jydm$%'</isNotEmpty> ) t
    </select>  
