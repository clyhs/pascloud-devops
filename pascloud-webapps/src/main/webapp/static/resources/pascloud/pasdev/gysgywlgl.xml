  <javascript> 
    <import>parent.addTab('gysgywldr','柜员手工业务量管理','/module/parser/import/visitHtml/gysgywldr');</import>  
    <query>jQuery(document).ready(function () { }); //自定义验证，在提交时执行。通过返回true，否则返回false function customValidate(){ var obj_tjrq = createTimeObj($('#tjrq').val()+'01'); $('#tjrq_ym').val(obj_tjrq.ym); return true; }</query>  
    <edit>var flag=true; jQuery(document).ready(function () { //这里的js脚本，会在页面加载完成后执行 jQuery('#ywl').attr('styleClass','min-value-$0'); jQuery('#jymc').attr("disabled",'true'); jQuery('#jydm').executeOnEvent('blur', 'gysgywlgl_checkJydm',setJymc); if($('#page_operate').val() == 'update'){ $('#tjrq').attr('disabled',true); } }); //自定义验证，在提交时执行。通过返回true，否则返回false function customValidate(){ var obj_tjrq = createTimeObj($('#tjrq').val()+'01'); $('#tjrq_ym').val(obj_tjrq.ym); if(flag==true){ return true; } } function setJymc (obj){ if(jQuery('#jydm').val()!=''){ if(obj.data.length&gt;0){ jQuery("#jymc").val(obj.data[0].JYMC); jQuery("#jydm").removeClass("error"); jQuery('label[class=error]').remove(); flag=true; }else{ jQuery("#jydm").addClass("error"); jQuery("#jydm").parent().append(" &lt;label for='jydm' generated='generated' class='error' style='display: block;'&gt;交易代码不存在&lt;/label&gt;"); flag=false; } } }</edit> 
