},{"id":1497510936669,"type":"DataInterface","propertyPanel":"datainterface","text":"(dataInterface:select)数据访问组件","row":"1","col":"3","rowspan":"1","colspan":"1","name":"getPfjsts","dataType":"select","sql":"select csz from xtb_xtcs where csmc=\\'PFJSTS\\'"},{"id":1497510939478,"type":"Hidden","propertyPanel":"hidden","text":"(xtrq:)隐藏域","row":"1","col":"3","rowspan":"1","colspan":"1","name":"pfjsts","label":"","defVal":""}]}','saveFunctionName':'行员在线评分进度查询','fileName':'hyzxpfjdcx','fnuctionId':'hyzxpfjdcx','scripts':'"jQuery(document).ready(function () {<br/>  BindParent(\\'famc\\',\\'khnf\\',\\'blur\\');<br/>  PAS.di.execute(\\'hyzxpfjdcx_getPfjsts\\',getPfjsts);<br/>  setTimeout(function () {<br/>  $(\"[name=\\'pfr_mc\\']\").val(\\'\\');<br/>  $(\"[name=\\'pfr\\']\").val(\\'\\');<br/><br/>  $(\"#query_table tr:eq(1) \").append($(\"#query_table tr:eq(2)\").find(\"td:last\"));<br/>  $(\\'#khzq\\').change(function(){<br/>     var xtrq = $(\\'#xtrq\\').val();<br/>     var pfts = $(\\'#pfjsts\\').val();<br/>     var qs = minusDate(xtrq,pfts);<br/>     if($(\\'#khzq\\').val()==\\'1\\'){<br/>     $(\\'#khqs\\').empty();<br/>     for (i = 1; i < 13; i++) {<br/>        if (qs != 0 && i == qs) {<br/>            $(\\'#khqs\\').append(\"<option value=\\'\"+i+\"\\' selected=\\'selected\\'>\" + i + \"月</option>\"); <br/>        } else {<br/>            $(\\'#khqs\\').append(\"<option value=\\'\"+i+\"\\'>\" + i + \"月</option>\"); <br/>        }<br/>     }<br/>   }else if($(\\'#khzq\\').val()==\\'2\\'){<br/>     $(\\'#khqs\\').empty();<br/>     for(i=1;i<5;i++){<br/>        var qs2 = qs/3;<br/>        if (qs2 != 0 && i == qs2) {<br/>            $(\\'#khqs\\').append(\"<option value=\\'\"+i+\"\\' selected=\\'selected\\'>第\"+i+\"季度</option>\");<br/>        } else {<br/>            $(\\'#khqs\\').append(\"<option value=\\'\"+i+\"\\'>第\"+i+\"季度</option>\");<br/>        }<br/>     }<br/>   }else{<br/>     $(\\'#khqs\\').empty();<br/>     $(\\'#khqs\\').append(\"<option value=\\'\"+$(\\'#khnf\\').val()+\"\\'>全年</option>\");<br/>     <br/>   }<br/> }).trigger(\\'change\\');},200);<br/>});<br/><br/><br/>//得到减轻评分天数的月份<br/>function minusDate(data,day){<br/>   var ts = data.substr(6,2);<br/>   var month =1;<br/>   //如果日期天数大于评分天数，说明去当前月的。反正取上个月的月份<br/>   if(parseInt(ts)>day){<br/>      month = data.substr(4,2);<br/>   }else{<br/>       if(data.substr(4,2) == \\'01\\'){<br/>           month = 12;<br/>           //年份应该减一<br/>          var nf= data.substr(0,4);<br/>          $(\\'#khnf\\').val(nf-1);<br/>       }else{<br/>          month = data.substr(4,2)-1;<br/>       }<br/>   }<br/>  return month;<br/>}<br/><br/>//得到评分结束天数<br/>function getPfjsts(obj){<br/>    if(obj.data.length>0){<br/>        var jsts=obj.data[0].CSZ;<br/>        $(\"#pfjsts\").val(jsts);<br/>    }<br/>    PAS.di.setDone(obj.sqlid);<br/>}<br/><br/>//自定义验证，在提交时执行。通过返回true，否则返回false<br/>function customValidate(){<br/> return true;<br/>}<br/><br/><br/>function customOnLoadSuccess(data){<br/>     highLightDataGride(data,compare,{color:\\'red\\'},\\'PFZT\\');<br/>    createGridDataListBar(data);<br/>}<br/><br/>function compare(row){<br/>   var pfzt = row[\\'PFZT\\'];<br/>    return  pfzt==\\'未完成\\';<br/> }"','plotJson':'[]','rslJson':'{"resultShowLogics":[]}'}]