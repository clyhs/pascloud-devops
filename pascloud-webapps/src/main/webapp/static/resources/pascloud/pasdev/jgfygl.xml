      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> insert into SGLR_JGFY( FYZL, JE, BZ, KHDXDH, CZSJ, CZR, TJRQ, KMH, BZSM ) values ( '$fyzl$',$je$,'$bz$',$khdxdh$,f_get_timestamp('$czsj$'),'$czr$',$tjrq_ym$,'$kmzl$','$bzsm$')
