  <sqlMap namespace="studio.jgfygl"> 
    <select id="dn0.jgfyglList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select $tjrq$ tjny,jg.jgdh,jg.jgmc,fy.fyzl,ms.dmms fyzlmc,fy.kmh,km.kmmc,fy.je,jg.khdxdh,fy.tjrq,fy.bzsm from sglr_jgfy fy inner join khdx_jg jg on jg.khdxdh=fy.khdxdh left join csb_dmms ms on ms.dmmc='FYZL' and ms.dmz=fy.fyzl left join csb_km km on km.kmh=fy.kmh inner join table(fn_getsjjg_java_qx($tjrq_ym$,$login_khdxdh$,0)) qx on qx.khdxdh=jg.khdxdh 
      <isNotEmpty property="jgkhdxdh">where fy.khdxdh in ($jgkhdxdh$)</isNotEmpty>  
      <isNotEmpty property="fyzl">and fy.fyzl='$fyzl$'</isNotEmpty>  
      <isNotEmpty property="tjrq_ym">and fy.tjrq=$tjrq_ym$</isNotEmpty> 
    </select>  
    <select id="dn0.jgfyglCount" parameterClass="java.util.Map" resultClass="java.lang.Integer"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select count(1) from ( select $tjrq$ tjny,jg.jgdh,jg.jgmc,fy.fyzl,ms.dmms fyzlmc,fy.kmh,km.kmmc,fy.je,jg.khdxdh,fy.tjrq,fy.bzsm from sglr_jgfy fy inner join khdx_jg jg on jg.khdxdh=fy.khdxdh left join csb_dmms ms on ms.dmmc='FYZL' and ms.dmz=fy.fyzl left join csb_km km on km.kmh=fy.kmh inner join table(fn_getsjjg_java_qx($tjrq_ym$,$login_khdxdh$,0)) qx on qx.khdxdh=jg.khdxdh 
      <isNotEmpty property="jgkhdxdh">where fy.khdxdh in ($jgkhdxdh$)</isNotEmpty>  
      <isNotEmpty property="fyzl">and fy.fyzl='$fyzl$'</isNotEmpty>  
      <isNotEmpty property="tjrq_ym">and fy.tjrq=$tjrq_ym$</isNotEmpty> ) t
    </select>  
    <select id="dn0.jgfygl_select_by_primaryKey" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select KMH, FYZL, KHDXDH, BZSM, JE,F_TIMETOCHAR(CZSJ,'yyyy-mm-dd hh24:mi:ss') as CZSJ, CZR, BZ, TJRQ from SGLR_JGFY where 1= 1 and FYZL = '$fyzl$' and KHDXDH = $khdxdh$ and TJRQ = $tjrq$
    </select>  
