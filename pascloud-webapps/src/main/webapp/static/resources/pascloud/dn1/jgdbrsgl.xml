    <edit>jQuery(document).ready(function () { //这里的js脚本，会在页面加载完成后执行 //PAS.di.execute('hyzjywsrgl_selectCzsj',selectCzsj); if($("#page_operate").val()=='update'){ $('#tjrq').attr("disabled","disabled"); } }); //自定义验证，在提交时执行。通过返回true，否则返回false function customValidate(){ var o_qsrq = createTimeObj(jQuery('#tjrq').val()); jQuery('#tjrq_ym').val(o_qsrq.ym); return true; } function selectCzsj(obj){ if(obj.data.length&gt;0){ $("#czsj").val(obj.data[0].CZSJ); } }</edit> 
