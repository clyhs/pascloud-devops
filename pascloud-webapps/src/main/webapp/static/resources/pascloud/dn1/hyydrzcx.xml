  <sqlMap namespace="studio.hyydrzcx"> 
    <select id="dn1.hyydrzcxList" parameterClass="java.util.Map" resultClass="java.util.HashMap" remapResults="true"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select rz.jldh,f_timetoint(rz.jlsj,'yyyymmdd') as jlsj,hy.hymc,jg1.jgmc as xgqjgmc,jg2.jgmc as xghjgmc ,rz.xgqsxrq,rz.xghsxrq,case when rz.czlx = '01' then '新增' when rz.czlx='02' then '修改' end as czlx , hy2.hymc as xgrmc from xtb_jgcyxgrz rz inner join khdx_jg jg1 on jg1.khdxdh = rz.xgqssjg inner join khdx_jg jg2 on jg2.khdxdh = rz.xghssjg inner join khdx_hy hy on hy.khdxdh = rz.khdxdh inner join khdx_hy hy2 on hy2.khdxdh = rz.xgrdh where 1=1 
      <isNotEmpty property="jgkhdxdh">and rz.xghssjg in ($jgkhdxdh$)</isNotEmpty>  
      <isNotEmpty property="khdxdh">and rz.khdxdh in ($khdxdh$)</isNotEmpty> order by jldh
    </select>  
    <select id="dn1.hyydrzcxCount" parameterClass="java.util.Map" resultClass="java.lang.Integer"> 
      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select count(1) from ( select rz.jldh,f_timetoint(rz.jlsj,'yyyymmdd') as jlsj,hy.hymc,jg1.jgmc as xgqjgmc,jg2.jgmc as xghjgmc ,rz.xgqsxrq,rz.xghsxrq,case when rz.czlx = '01' then '新增' when rz.czlx='02' then '修改' end as czlx , hy2.hymc as xgrmc from xtb_jgcyxgrz rz inner join khdx_jg jg1 on jg1.khdxdh = rz.xgqssjg inner join khdx_jg jg2 on jg2.khdxdh = rz.xghssjg inner join khdx_hy hy on hy.khdxdh = rz.khdxdh inner join khdx_hy hy2 on hy2.khdxdh = rz.xgrdh where 1=1 
      <isNotEmpty property="jgkhdxdh">and rz.xghssjg in ($jgkhdxdh$)</isNotEmpty>  
      <isNotEmpty property="khdxdh">and rz.khdxdh in ($khdxdh$)</isNotEmpty> order by jldh ) t
