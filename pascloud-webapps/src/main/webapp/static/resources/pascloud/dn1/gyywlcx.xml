      <isNotEmpty property="db"><![CDATA[/**!mycat:datanode=$db$**/]]></isNotEmpty> select $tjrq$ as tjrq,jg.jgdh,jg.jgmc,hy.hydh,hy.hymc,hy.khdxdh , sum(ywl.xtzqywl) zsqywl,sum(ywl.xtzhywl) zshywl,sum(ywl.bcywl) bcywl from NBZZ_HYKH_YWL ywl inner join khdx_jgcy cy on ywl.khdxdh=cy.khdxdh inner join khdx_jg jg on cy.jgkhdxdh=jg.khdxdh inner join khdx_hy hy on ywl.khdxdh=hy.khdxdh where 1=1 
      <isNotEmpty property="tjrq">and ywl.tjrq between $tjrq$01 and $tjrq$31</isNotEmpty>  
